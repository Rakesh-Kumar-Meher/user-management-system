# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Atlas Connection
# Get your connection string from MongoDB Atlas
MONGO_URI=mongodb+srv://username:password@cluster.xxxxx.mongodb.net/databaseName?retryWrites=true&w=majority

# JWT Secret
# Generate using: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=your_generated_jwt_secret_key_here_minimum_64_characters
